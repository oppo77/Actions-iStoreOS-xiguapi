CONFIG_BOOTDELAY=3
CONFIG_USE_PREBOOT=y
CONFIG_PREBOOT="echo; echo Welcome to NLNET Xiguapi V3; echo"

# 引导命令配置
CONFIG_BOOTCOMMAND="if mmc dev 1 && mmc dev; then " \
                   "setenv devnum 1; setenv devtype mmc; " \
                   "elif mmc dev 0 && mmc dev; then " \
                   "setenv devnum 0; setenv devtype mmc; " \
                   "else setenv devtype mmc; setenv devnum 0; fi; " \
                   "if ${devtype} dev ${devnum}; then " \
                   "part start ${devtype} ${devnum} 1 kernel_start; " \
                   "part size ${devtype} ${devnum} 1 kernel_size; " \
                   "setexpr kernel_addr_r ${kernel_start} + 0; " \
                   "setexpr fdt_addr_r ${kernel_start} + 0x800000; " \
                   "setexpr ramdisk_addr_r ${kernel_start} + 0x1000000; " \
                   "${devtype} read ${kernel_addr_r} ${kernel_start} ${kernel_size}; " \
                   "bootm ${kernel_addr_r} ${ramdisk_addr_r} ${fdt_addr_r}; " \
                   "fi; reset"

# 引导参数
CONFIG_BOOTARGS="console=ttyS2,115200n8 earlycon=uart8250,mmio32,0xfe660000 " \
                "root=/dev/mmcblk0p2 rootwait rw " \
                "cma=16M@0x40000000-0x41000000 " \
                "clk_ignore_unused " \
                "loglevel=7"

