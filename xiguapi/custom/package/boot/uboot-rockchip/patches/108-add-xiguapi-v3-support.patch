diff -Naur u-boot-origin/arch/arm/dts/rk3568-xiguapi-v3.dts u-boot-modified/arch/arm/dts/rk3568-xiguapi-v3.dts
--- u-boot-origin/arch/arm/dts/rk3568-xiguapi-v3.dts	1970-01-01 08:00:00.000000000 +0800
+++ u-boot-modified/arch/arm/dts/rk3568-xiguapi-v3.dts	2025-12-08 04:50:44.437164000 +0800
@@ -0,0 +1,653 @@
+// SPDX-License-Identifier: (GPL-2.0+ OR MIT)
+/*
+ * U-Boot device tree for xiguapi-v3 based on RK3568
+ * Linux kernel: 6.6.110
+ * U-Boot version: 24.10
+ * Configuration: Minimal for U-Boot booting with USB device mode (fastboot)
+ * Hardware: xiguapi-v3 board with RK3568, 4GB LPDDR4X, 32GB eMMC
+ */
+
+/dts-v1/;
+#include "rk3568-u-boot.dtsi"
+
+/ {
+	model = "xiguapi-v3 (RK3568)";
+	compatible = "rockchip,xiguapi-v3", "rockchip,rk3568";
+
+	aliases {
+		mmc0 = &sdhci;      // eMMC (32GB)
+		mmc1 = &sdmmc0;     // SD card
+		mmc2 = &sdmmc2;     // TF card (备用启动)
+		serial2 = &uart2;   // Debug UART (UART2)
+		serial9 = &uart9;   // 5G模块串口
+		spi0 = &sfc;        // SPI Flash控制器
+		i2c0 = &i2c0;       // PMIC I2C
+		i2c2 = &i2c2;       // 扩展I2C
+		ethernet0 = &gmac1; // 以太网0
+		ethernet1 = &gmac1; // 以太网1 (双PHY)
+	};
+
+	chosen {
+		stdout-path = "serial2:115200n8";
+		u-boot,spl-boot-order = "same-as-spl", &sdmmc0, &sdhci, &sdmmc2;
+	};
+
+	// 内存配置 - 4GB LPDDR4X @ 0x80000000
+	memory@0 {
+		device_type = "memory";
+		reg = <0x0 0x80000000 0x0 0x100000000>;
+	};
+
+	// 保留内存区域
+	reserved-memory {
+		#address-cells = <2>;
+		#size-cells = <2>;
+		ranges;
+
+		// 多媒体保留内存（VPU/GPU）- 64MB
+		vpu_reserved: vpu@88000000 {
+			reg = <0x0 0x88000000 0x0 0x04000000>;
+			no-map;
+		};
+
+		// DMA缓冲区区域 - 32MB
+		dma_reserved: dma@8c000000 {
+			reg = <0x0 0x8c000000 0x0 0x02000000>;
+			compatible = "shared-dma-pool";
+			reusable;
+			no-map;
+		};
+	};
+
+	// 5G模块专用3.8V电源（外部DC-DC）
+	vcc3v8_5g: regulator-5g {
+		compatible = "ti,tps62172-regulator";
+		regulator-name = "vcc3v8_5g";
+		regulator-min-microvolt = <3750000>;
+		regulator-max-microvolt = <3850000>;
+		regulator-always-off; // 默认关闭，由软件控制
+		status = "okay";
+	};
+
+	// 固定时钟定义
+	clocks {
+		// 以太网参考时钟 - 25MHz
+		eth_ref_clk: eth-ref-clk {
+			compatible = "fixed-clock";
+			clock-frequency = <25000000>;
+			clock-output-names = "eth_ref_clk";
+			#clock-cells = <0>;
+		};
+
+		// SPI LCD时钟 - 20MHz
+		spi_lcd_clk: spi-lcd-clk {
+			compatible = "fixed-clock";
+			clock-frequency = <20000000>;
+			clock-output-names = "spi_lcd_clk";
+			#clock-cells = <0>;
+		};
+	};
+};
+
+// 调试串口配置 - UART2 @ 115200 bps
+&uart2 {
+	status = "okay";
+	pinctrl-names = "default";
+	pinctrl-0 = <&uart2m0_xfer>;
+};
+
+// 5G模块串口 - UART9 @ 4Mbps
+&uart9 {
+	status = "disabled"; // U-Boot中默认禁用
+	pinctrl-names = "default";
+	pinctrl-0 = <&uart9m1_xfer>;
+};
+
+// eMMC配置 - 32GB SanDisk SDINBDA4-32G
+&sdhci {
+	bus-width = <8>;
+	cap-mmc-highspeed;
+	mmc-hs200-1_8v;
+	no-sd;
+	no-sdio;
+	non-removable;
+	pinctrl-names = "default";
+	pinctrl-0 = <&emmc_bus8 &emmc_clk &emmc_cmd &emmc_datastrobe>;
+	status = "okay";
+};
+
+// SD卡配置 - 主启动设备
+&sdmmc0 {
+	bus-width = <4>;
+	cap-sd-highspeed;
+	disable-wp;
+	no-mmc;
+	no-sdio;
+	pinctrl-names = "default";
+	pinctrl-0 = <&sdmmc0_bus4 &sdmmc0_clk &sdmmc0_cmd &sdmmc0_det>;
+	status = "okay";
+};
+
+// TF卡配置 - 备用启动设备
+&sdmmc2 {
+	bus-width = <4>;
+	cap-sd-highspeed;
+	disable-wp;
+	no-mmc;
+	no-sdio;
+	pinctrl-names = "default";
+	pinctrl-0 = <&sdmmc2_bus4 &sdmmc2_clk &sdmmc2_cmd &sdmmc2_det>;
+	status = "okay";
+};
+
+// SPI Flash控制器
+&sfc {
+	#address-cells = <1>;
+	#size-cells = <0>;
+	status = "okay";
+
+	flash@0 {
+		compatible = "jedec,spi-nor";
+		reg = <0>;
+		spi-max-frequency = <24000000>;
+	};
+};
+
+// USB3.0 OTG0 - 配置为设备模式用于fastboot
+&usb_host0_xhci {
+	dr_mode = "peripheral";
+	status = "okay";
+};
+
+// USB PHY配置
+&usb2phy0 {
+	status = "okay";
+};
+
+&usb2phy0_otg {
+	status = "okay";
+};
+
+// USB2.0 HOST - 禁用（U-Boot不需要）
+&usb_host1_ehci {
+	status = "disabled";
+};
+
+&usb_host1_ohci {
+	status = "disabled";
+};
+
+// 以太网GMAC1 - 简化配置
+&gmac1 {
+	phy-mode = "rgmii-id";
+	clock_in_out = "input";
+	snps,reset-gpio = <&gpio3 RK_PC1 GPIO_ACTIVE_LOW>;
+	snps,reset-active-low;
+	snps,reset-delays-us = <0 20000 100000>;
+	assigned-clocks = <&cru SCLK_GMAC1_RX_TX>, <&cru SCLK_GMAC1>;
+	assigned-clock-parents = <&cru SCLK_GMAC1_RGMII_SPEED>;
+	assigned-clock-rates = <0>, <125000000>;
+	pinctrl-names = "default";
+	pinctrl-0 = <&gmac1m0_miim &gmac1m0_tx_bus2 &gmac1m0_rx_bus2
+		     &gmac1m0_rgmii_clk &gmac1m0_rgmii_bus>;
+	status = "okay";
+};
+
+// PMIC (RK817) - U-Boot电源管理需要
+&i2c0 {
+	status = "okay";
+	clock-frequency = <400000>;
+
+	pmic: pmic@18 {
+		compatible = "rockchip,rk817";
+		reg = <0x18>;
+
+		// 电源通道配置
+		regulators {
+			vcc0v9_ddr: DCDC_REG1 {
+				regulator-name = "vcc0v9_ddr";
+				regulator-min-microvolt = <900000>;
+				regulator-max-microvolt = <900000>;
+				regulator-always-on;
+				regulator-boot-on;
+			};
+
+			vcc1v2_cpu: DCDC_REG2 {
+				regulator-name = "vcc1v2_cpu";
+				regulator-min-microvolt = <1200000>;
+				regulator-max-microvolt = <1200000>;
+				regulator-always-on;
+				regulator-boot-on;
+			};
+
+			vcc1v8_peri: DCDC_REG3 {
+				regulator-name = "vcc1v8_peri";
+				regulator-min-microvolt = <1800000>;
+				regulator-max-microvolt = <1800000>;
+				regulator-always-on;
+				regulator-boot-on;
+			};
+
+			vcc3v3_peri: LDO_REG1 {
+				regulator-name = "vcc3v3_peri";
+				regulator-min-microvolt = <3300000>;
+				regulator-max-microvolt = <3300000>;
+				regulator-always-on;
+				regulator-boot-on;
+			};
+
+			vcc4v2_pre: LDO_REG2 {
+				regulator-name = "vcc4v2_pre";
+				regulator-min-microvolt = <4200000>;
+				regulator-max-microvolt = <4200000>;
+				regulator-always-on;
+				regulator-boot-on;
+			};
+
+			vcc5v_usb: LDO_REG3 {
+				regulator-name = "vcc5v_usb";
+				regulator-min-microvolt = <5000000>;
+				regulator-max-microvolt = <5000000>;
+				regulator-always-on;
+				regulator-boot-on;
+			};
+		};
+	};
+};
+
+// 扩展I2C2 - 用于外设（GC9307显示屏等）
+&i2c2 {
+	status = "disabled"; // U-Boot中默认禁用
+	clock-frequency = <100000>;
+
+	// GC9307显示屏驱动（地址0x36）
+	gc9307: gc9307@36 {
+		compatible = "ilitek,gc9307";
+		reg = <0x36>;
+		status = "disabled";
+	};
+};
+
+// SPI主机 - 用于GC9307显示屏
+&spi1 {
+	status = "disabled"; // U-Boot中默认禁用
+	pinctrl-names = "default";
+	pinctrl-0 = <&spi1m0_cs0 &spi1m0_pins>;
+	#address-cells = <1>;
+	#size-cells = <0>;
+
+	gc9307_spi: lcd@0 {
+		compatible = "ilitek,gc9307";
+		reg = <0>;
+		spi-max-frequency = <20000000>;
+		status = "disabled";
+	};
+};
+
+// PWM5 - LED调光（1KHz）
+&pwm5 {
+	status = "disabled"; // U-Boot中默认禁用
+	pinctrl-names = "default";
+	pinctrl-0 = <&pwm5m1_pins>;
+};
+
+// PWM14 - 风扇调速（25KHz）
+&pwm14 {
+	status = "disabled"; // U-Boot中默认禁用
+	pinctrl-names = "default";
+	pinctrl-0 = <&pwm14m0_pins>;
+};
+
+// SARADC - 温度检测
+&saradc {
+	status = "okay"; // 可用于温度检测
+	vref-supply = <&vcc3v3_peri>;
+};
+
+// PCIe控制器 - 禁用（U-Boot不需要）
+&pcie2x1 {
+	status = "disabled";
+};
+
+&pcie30phy {
+	status = "disabled";
+};
+
+&pcie3x1 {
+	status = "disabled";
+};
+
+&pcie3x2 {
+	status = "disabled";
+};
+
+// HDMI - 禁用
+&hdmi {
+	status = "disabled";
+};
+
+&hdmi_in_vp0 {
+	status = "disabled";
+};
+
+&hdmi_in_vp1 {
+	status = "disabled";
+};
+
+&hdmi_sound {
+	status = "disabled";
+};
+
+// MIPI显示接口 - 禁用
+&dsi {
+	status = "disabled";
+};
+
+&dsi_in_vp0 {
+	status = "disabled";
+};
+
+&dsi_in_vp1 {
+	status = "disabled";
+};
+
+// 视频处理单元 - 禁用
+&vop {
+	status = "disabled";
+};
+
+&vp0 {
+	status = "disabled";
+};
+
+&vp1 {
+	status = "disabled";
+};
+
+&vp2 {
+	status = "disabled";
+};
+
+&vp3 {
+	status = "disabled";
+};
+
+// 视频编解码器 - 禁用
+&vepu {
+	status = "disabled";
+};
+
+&vdpu {
+	status = "disabled";
+};
+
+&vdec {
+	status = "disabled";
+};
+
+&venc {
+	status = "disabled";
+};
+
+// GPU - 禁用
+&gpu {
+	status = "disabled";
+};
+
+// NPU - 禁用
+&npu {
+	status = "disabled";
+};
+
+&npu_opp_table {
+	status = "disabled";
+};
+
+// 引脚控制器配置
+&pinctrl {
+	// UART2引脚配置
+	uart2 {
+		uart2m0_xfer: uart2m0-xfer {
+			rockchip,pins = 
+				<1 RK_PA6 RK_FUNC_1 &pcfg_pull_none>, // TX
+				<1 RK_PA7 RK_FUNC_1 &pcfg_pull_none>; // RX
+		};
+	};
+
+	// UART9引脚配置
+	uart9 {
+		uart9m1_xfer: uart9m1-xfer {
+			rockchip,pins = 
+				<2 RK_PB2 RK_FUNC_2 &pcfg_pull_none>, // TX
+				<2 RK_PB3 RK_FUNC_2 &pcfg_pull_none>; // RX
+		};
+	};
+
+	// eMMC引脚配置
+	emmc {
+		emmc_bus8: emmc-bus8 {
+			rockchip,pins = 
+				<1 RK_PC0 RK_FUNC_2 &pcfg_pull_up_drv_level_2>,
+				<1 RK_PC1 RK_FUNC_2 &pcfg_pull_up_drv_level_2>,
+				<1 RK_PC2 RK_FUNC_2 &pcfg_pull_up_drv_level_2>,
+				<1 RK_PC3 RK_FUNC_2 &pcfg_pull_up_drv_level_2>,
+				<1 RK_PC4 RK_FUNC_2 &pcfg_pull_up_drv_level_2>,
+				<1 RK_PC5 RK_FUNC_2 &pcfg_pull_up_drv_level_2>,
+				<1 RK_PC6 RK_FUNC_2 &pcfg_pull_up_drv_level_2>,
+				<1 RK_PC7 RK_FUNC_2 &pcfg_pull_up_drv_level_2>;
+		};
+
+		emmc_clk: emmc-clk {
+			rockchip,pins = <1 RK_PB7 RK_FUNC_2 &pcfg_pull_up_drv_level_2>;
+		};
+
+		emmc_cmd: emmc-cmd {
+			rockchip,pins = <1 RK_PB6 RK_FUNC_2 &pcfg_pull_up_drv_level_2>;
+		};
+
+		emmc_datastrobe: emmc-datastrobe {
+			rockchip,pins = <1 RK_PB5 RK_FUNC_2 &pcfg_pull_up_drv_level_2>;
+		};
+	};
+
+	// SD卡引脚配置
+	sdmmc0 {
+		sdmmc0_bus4: sdmmc0-bus4 {
+			rockchip,pins = 
+				<2 RK_PB4 RK_FUNC_1 &pcfg_pull_up_drv_level_2>,
+				<2 RK_PB5 RK_FUNC_1 &pcfg_pull_up_drv_level_2>,
+				<2 RK_PB6 RK_FUNC_1 &pcfg_pull_up_drv_level_2>,
+				<2 RK_PB7 RK_FUNC_1 &pcfg_pull_up_drv_level_2>;
+		};
+
+		sdmmc0_clk: sdmmc0-clk {
+			rockchip,pins = <2 RK_PC0 RK_FUNC_1 &pcfg_pull_up_drv_level_2>;
+		};
+
+		sdmmc0_cmd: sdmmc0-cmd {
+			rockchip,pins = <2 RK_PC1 RK_FUNC_1 &pcfg_pull_up_drv_level_2>;
+		};
+
+		sdmmc0_det: sdmmc0-det {
+			rockchip,pins = <0 RK_PA4 RK_FUNC_1 &pcfg_pull_none>;
+		};
+	};
+
+	// TF卡引脚配置
+	sdmmc2 {
+		sdmmc2_bus4: sdmmc2-bus4 {
+			rockchip,pins = 
+				<4 RK_PA0 RK_FUNC_1 &pcfg_pull_up_drv_level_2>,
+				<4 RK_PA1 RK_FUNC_1 &pcfg_pull_up_drv_level_2>,
+				<4 RK_PA2 RK_FUNC_1 &pcfg_pull_up_drv_level_2>,
+				<4 RK_PA3 RK_FUNC_1 &pcfg_pull_up_drv_level_2>;
+		};
+
+		sdmmc2_clk: sdmmc2-clk {
+			rockchip,pins = <4 RK_PA4 RK_FUNC_1 &pcfg_pull_up_drv_level_2>;
+		};
+
+		sdmmc2_cmd: sdmmc2-cmd {
+			rockchip,pins = <4 RK_PA5 RK_FUNC_1 &pcfg_pull_up_drv_level_2>;
+		};
+
+		sdmmc2_det: sdmmc2-det {
+			rockchip,pins = <4 RK_PA6 RK_FUNC_1 &pcfg_pull_none>;
+		};
+	};
+
+	// 以太网GMAC1引脚配置
+	gmac1 {
+		gmac1m0_miim: gmac1m0-miim {
+			rockchip,pins = 
+				<3 RK_PC3 RK_FUNC_2 &pcfg_pull_none>,
+				<3 RK_PC4 RK_FUNC_2 &pcfg_pull_none>;
+		};
+
+		gmac1m0_tx_bus2: gmac1m0-tx-bus2 {
+			rockchip,pins = 
+				<3 RK_PC6 RK_FUNC_2 &pcfg_pull_none_drv_level_3>,
+				<3 RK_PC7 RK_FUNC_2 &pcfg_pull_none_drv_level_3>,
+				<3 RK_PD0 RK_FUNC_2 &pcfg_pull_none_drv_level_3>;
+		};
+
+		gmac1m0_rx_bus2: gmac1m0-rx-bus2 {
+			rockchip,pins = 
+				<3 RK_PD1 RK_FUNC_2 &pcfg_pull_none>,
+				<3 RK_PD2 RK_FUNC_2 &pcfg_pull_none>,
+				<3 RK_PD3 RK_FUNC_2 &pcfg_pull_none>;
+		};
+
+		gmac1m0_rgmii_clk: gmac1m0-rgmii-clk {
+			rockchip,pins = 
+				<3 RK_PD5 RK_FUNC_2 &pcfg_pull_none>,
+				<3 RK_PD4 RK_FUNC_2 &pcfg_pull_none>;
+		};
+
+		gmac1m0_rgmii_bus: gmac1m0-rgmii-bus {
+			rockchip,pins = 
+				<3 RK_PD7 RK_FUNC_2 &pcfg_pull_none_drv_level_3>,
+				<3 RK_PD6 RK_FUNC_2 &pcfg_pull_none_drv_level_3>;
+		};
+	};
+
+	// SPI1引脚配置（用于显示屏）
+	spi1 {
+		spi1m0_cs0: spi1m0-cs0 {
+			rockchip,pins = <3 RK_PA0 RK_FUNC_0 &pcfg_pull_up>;
+		};
+
+		spi1m0_pins: spi1m0-pins {
+			rockchip,pins = 
+				<3 RK_PA1 RK_FUNC_1 &pcfg_pull_none_drv_level_3>, // CLK
+				<3 RK_PA2 RK_FUNC_1 &pcfg_pull_none_drv_level_3>; // MOSI
+		};
+	};
+
+	// PWM5引脚配置
+	pwm5 {
+		pwm5m1_pins: pwm5m1-pins {
+			rockchip,pins = <1 RK_PC1 RK_FUNC_2 &pcfg_pull_none>;
+		};
+	};
+
+	// PWM14引脚配置
+	pwm14 {
+		pwm14m0_pins: pwm14m0-pins {
+			rockchip,pins = <3 RK_PD6 RK_FUNC_1 &pcfg_pull_none>;
+		};
+	};
+
+	// 按键引脚配置
+	keys {
+		reset_key: reset-key {
+			rockchip,pins = <0 RK_PA0 RK_FUNC_0 &pcfg_pull_up>;
+		};
+
+		flash_key: flash-key {
+			rockchip,pins = <4 RK_PD2 RK_FUNC_0 &pcfg_pull_up>;
+		};
+
+		power_key: power-key {
+			rockchip,pins = <0 RK_PA1 RK_FUNC_0 &pcfg_pull_up>;
+		};
+	};
+
+	// 5G模块电源控制引脚
+	gpio4 {
+		// 5G模块电源使能引脚
+		vdd_main_en: vdd-main-en {
+			rockchip,pins = <4 RK_PA0 RK_FUNC_GPIO &pcfg_pull_none>;
+		};
+
+		vdd_io_en: vdd-io-en {
+			rockchip,pins = <4 RK_PA1 RK_FUNC_GPIO &pcfg_pull_none>;
+		};
+
+		vdd_rf_en: vdd-rf-en {
+			rockchip,pins = <4 RK_PA2 RK_FUNC_GPIO &pcfg_pull_none>;
+		};
+
+		fiveg_reset: fiveg-reset {
+			rockchip,pins = <4 RK_PA3 RK_FUNC_GPIO &pcfg_pull_none>;
+		};
+	};
+};
+
+// 时钟控制器
+&cru {
+	assigned-clocks = <&cru PLL_GPLL>, <&cru PLL_CPLL>,
+			  <&cru PLL_VPLL>, <&cru PLL_NPLL>,
+			  <&cru ACLK_BUS>, <&cru ACLK_PERIMID>,
+			  <&cru HCLK_BUS>, <&cru HCLK_PERIMID>,
+			  <&cru PCLK_BUS>, <&cru CLK_24M>;
+	assigned-clock-rates = <1188000000>, <1000000000>,
+			       <500000000>, <1188000000>,
+			       <150000000>, <150000000>,
+			       <150000000>, <150000000>,
+			       <100000000>, <24000000>;
+	status = "okay";
+};
+
+// 系统GRF
+&sys_grf {
+	status = "okay";
+};
+
+// PMU GRF
+&pmugrf {
+	status = "okay";
+};
